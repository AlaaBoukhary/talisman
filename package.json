  "devDependencies": {
    "@yomguithereal/eslint-config": "^2.1.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.4.0",
    "mocha": "^2.4.5"
  },
  "babel": {

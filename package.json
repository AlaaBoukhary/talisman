{
  "name": "talisman",
  "version": "0.9.0",
  "description": "A straightforward & modular NLP, machine learning & fuzzy matching library for JavaScript.",
  "main": "index.js",
  "scripts": {
    "check": "npm test && npm run lint",
    "clean": "rimraf $npm_package_talisman_folders",
    "dist": "npm run clean && babel src --out-dir ./",
    "lint": "eslint src test",
    "prepublish": "npm run dist",
    "postpublish": "npm run clean",
    "test": "mocha --compilers js:babel-core/register -R spec ./test/endpoint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yomguithereal/talisman.git"
  },
  "talisman": {
    "folders": "classification clustering features helpers inflectors metrics phonetics regex stats stemmers tokenizers"
  },
  "keywords": [
    "bayes",
    "canberra",
    "caverphone",
    "chebyshev",
    "cologne",
    "cosine",
    "classifier",
    "clustering",
    "daitch-mokotoff",
    "dice",
    "fuzzy",
    "hamming",
    "k-means",
    "jaccard",
    "jaro",
    "lancaster",
    "levenshtein",
    "machine learning",
    "metaphone",
    "mra",
    "natural language processing",
    "ngrams",
    "nlp",
    "nysiis",
    "perceptron",
    "phonetic",
    "porter",
    "punkt",
    "schinke",
    "sorensen",
    "soundex",
    "stats",
    "tfidf",
    "tokenizer",
    "tversky",
    "vectorizer",
    "winkler"
  ],
  "author": {
    "name": "Guillaume Plique",
    "url": "http://github.com/Yomguithereal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yomguithereal/talisman/issues"
  },
  "homepage": "https://github.com/yomguithereal/talisman#readme",
  "devDependencies": {
    "@yomguithereal/eslint-config": "^2.1.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-es2015-destructuring": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "csv": "^1.1.0",
    "eslint": "^2.4.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-es2015-destructuring",
        {
          "loose": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@yomguithereal/eslint-config/es6",
    "rules": {
      "no-return-assign": 0
    }
  },
  "dependencies": {
    "html-entities": "^1.2.0",
    "lodash": "^4.6.1",
    "set-functions": "^0.2.0"
  }
}
